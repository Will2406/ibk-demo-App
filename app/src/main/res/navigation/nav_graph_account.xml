<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_account"
    app:startDestination="@id/accountFragment">

    <fragment
        android:id="@+id/accountFragment"
        android:name="com.ibk.app.product.ui.AccountFragment"
        android:label="AccountFragment"
        tools:layout="@layout/fragment_account">
        <action
            android:id="@+id/action_accountFragment_to_detailAccountFragment"
            app:destination="@id/detailAccountFragment" />

    </fragment>
    <fragment
        android:id="@+id/detailAccountFragment"
        android:name="com.ibk.app.product.ui.DetailAccountFragment"
        android:label="DetailAccountFragment"
        tools:layout="@layout/fragment_detail_account" />

</navigation>